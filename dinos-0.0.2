var star = function(x, y, size, points){
    size=abs(size);
    points=abs(points);
    //rotate yellow triangle around center point
    for(var i = 0; i<360; i+=360/points){
            pushMatrix();
            translate(x, y);
            scale((size/100)*points/10);
            noStroke();
            rotate(i);
            triangle(-360/points, 0, 360/points, 0, 0, 360/(points/2));
            popMatrix();
        }
    };
    
